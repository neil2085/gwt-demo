<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:c="urn:import:com.google.gwt.user.cellview.client" xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>

	<g:DockLayoutPanel>
		<g:north size="200">
			<c:CellTable ui:field="cellTabView" />
		</g:north>
		<g:center>
			<g:VerticalPanel>
				<g:Button text="Add" ui:field="addButton" />
				<c:DataGrid ui:field="dataGridView" />
			</g:VerticalPanel>
		</g:center>

	</g:DockLayoutPanel>



</ui:UiBinder> 